    "sass": "^1.25.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.0.4",
    "axios": "^0.21.1",
    "bootstrap": "^4.4.1",
    "electron-store": "^5.1.0",
    "jquery": "^3.4.1",
    "node-cron": "^2.0.3",
    "popper.js": "^1.16.1",
    "socket.io-client": "^2.3.0"
